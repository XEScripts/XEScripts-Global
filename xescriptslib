local Neverlose = loadstring(game:HttpGet("Ð¢Ð’ÐžÐ™_RAW_URL"))()

local Window = Neverlose.new({
    Title = "ðŸ”¥ Neverlose Style Menu",
    Size = UDim2.new(0, 500, 0, 500)
})

local MainTab = Window:Tab("Main")
local VisualsTab = Window:Tab("Visuals")
local SettingsTab = Window:Tab("Settings")

-- Main Tab
MainTab:Label("Combat Features")
MainTab:Toggle("Aimbot", false, function(state)
    print("Aimbot:", state)
end)

MainTab:Slider("FOV", 1, 360, 90, function(value)
    print("FOV:", value)
end)

MainTab:Button("Teleport to Base", function()
    print("Teleporting...")
end)

-- Visuals Tab
VisualsTab:Toggle("ESP", false, function(state)
    print("ESP:", state)
end)

VisualsTab:Toggle("Wallhack", true, function(state)
    print("Wallhack:", state)
end)

-- Settings Tab
SettingsTab:Button("Save Config", function()
    print("Config saved!")
end)
